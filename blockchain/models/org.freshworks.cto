namespace org.freshworks

participant User identified by userId {
  o String    userId
  o String    firstName
  o String    lastName
}

asset SampleAsset identified by assetId {
  --> User    owner
  o String    assetId
  o String    value
}

transaction SampleTransaction {
  --> SampleAsset asset
  o String    newValue
}

event SampleEvent {
  --> SampleAsset asset
  o String    oldValue
  o String    newValue
}
